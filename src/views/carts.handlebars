<div class="container title">
    <p>carts</p>
</div>
<div class="container">
</div>
<div class="container">
    <ul class="list-group">
        <div class="d-flex flex-column ">
            {{#each products}}
            <li>
                <div class="d-flex" style="width: 500px">
                    <div>
                        <img src={{this.photo}} class="cart-img-top">
                    </div>
                    <div class="card-body">
                        <h5>{{this.title}}</h5>
                        <p class="card-text">{{this.price}}</p>
                    </div>
                </div>

            </li>
            {{/each}}
        </div>
    </ul>
</div>